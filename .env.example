# Database Configuration
# These variables are used by docker-compose.yml
DB_USER=postgres
DB_PASSWORD=your_secure_password_here
DB_NAME=DeepAzure

# pgAdmin Configuration
PGADMIN_EMAIL=admin@deepazure.com
PGADMIN_PASSWORD=your_pgadmin_password_here

# JWT Configuration (Optional - can also be set in appsettings.json)
# Generate with: openssl rand -base64 32
JWT_SECRET_KEY=your_jwt_secret_key_here_minimum_32_characters

# Application Settings
ASPNETCORE_ENVIRONMENT=Development
API_PORT=5000
API_PORT_HTTPS=5001
POSTGRES_PORT=5433
PGADMIN_PORT=5050
